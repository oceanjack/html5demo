<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"    
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="tank" class="cn.edu.njust.webai.bean.XmlProject">
		<property name="name" value="tank" />
		<property name="summary" value="A kind of classical chess game" />
		<property name="scale" value="2" />
		<property name="lang" value="Js" />
		<property name="scene" value="3D" />
		<property name="menuItems">
			<map>
				<entry key="snack" value="Rock" />
			</map>
		</property>
	</bean>
</beans>
